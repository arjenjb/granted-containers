        "@storybook/addon-links": "^6.0.26",
        "@storybook/addon-viewport": "^6.0.26",
        "@storybook/addons": "^6.0.26",
        "@storybook/preset-typescript": "^3.0.0",
        "@storybook/react": "^6.0.26",
        "@types/chrome": "^0.0.124",
        "@types/jest": "^26.0.14",
        "@types/node": "^14.11.8",
        "@types/react": "^16.9.52",
