{
    "name": "@commonfate/granted-containers",
    "version": "1.1.2",
    "description": "A Firefox extension to open cloud accounts in separate containers",
    "main": "index.js",
    "scripts": {
        "build:transpile": "webpack --config webpack.prod.js",
        "build:package": "web-ext build --source-dir ./dist/",
        "build": "run-s build:transpile build:package",
        "dev": "webpack -w --config webpack.dev.js",
        "test": "jest --config=jest.config.js",
        "lint": "eslint --fix -c ./.eslintrc.js \"src/**/*.ts*\"",
        "prettify": "prettier --write \"src/**/*.ts*\"",
        "storybook": "start-storybook -p 6006",
        "build-storybook": "build-storybook",
        "start:firefox": "web-ext run --source-dir ./dist/"
    },
    "keywords": [
        "react",
        "typescript",
        "chrome",
        "extension",
        "boilerplate"
    ],
    "author": "Common Fate",
    "license": "MIT",
    "homepage": "https://commonfate.io",
    "devDependencies": {
